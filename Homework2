X1 <- read_excel("C:/Users/shawn/Desktop/1.xlsx"
x<-ts(X1$price,start = c(1984,1),frequency = 12)
plot(x)                     # x is not stationary.(we can use plot,ACF or Augmented Dickey-Fuller Test)
# plot(x) 
# acf(x)
# adf.test(x)               # library('TSA')
(x[343]+x[344]+x[345])/3->x[346]  #data cleaning
y=log(x)
plot(y)  
# plot(y) 
# acf(y)
# adf.test(y)
y1 <- diff(y, differences=1)
y2 <- diff(y, differences=2)
